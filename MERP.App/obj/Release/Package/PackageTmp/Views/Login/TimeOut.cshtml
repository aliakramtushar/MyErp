<script type="text/javascript">
    $(document).ready(function () {
        MSWAL_WithReturnMSWAL('Timeout', 'your session time out, please login again', 'warning', true, true, return_fn);
    });
    function return_fn() {
        window.location.href = "/Login/ViewLogin";
    }
